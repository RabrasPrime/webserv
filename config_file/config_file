server
		listen 127.0.0.1
	server_name      
	root ./www/monsite
	client_max_body_size 4k
	error_pages 404 ./www/site1/errors/404.html
	error_pages 500 501 ./www/site1/errors/5c.html
	error_pages 404 ./www/site1/errors/404_new.html
	error_pages 403 ./www/site1/errors/403.html
	error_pages 501
	methods GET POST DELETE
	locations /images /picture
		root ./data/media
		methods GET POST
		auto_index True
	locations /api
		methods GET POST
		index default_page.html index.html
	locations /upload
		methods POST
		upload_path ./www/images
	locations /cgi-bin
		cgi_enabled true
		methods GET POST
		root ./www/script/list
		cgi_ext .py .tst /usr/bin/python3
		cgi_ext .sh /bin/bash
		cgi_ext .py off
		cgi_working_dir ./www/script/exec
		cgi_upload_path ./www/script/tmp
		cgi_timeout 10

server
	listen 127.0.0.:9090
	server_name test.local
	root ./www/test
	methods POST

server
	listen 80
	server_name site.com www.site.com sit.com www.sit.com
	root /var/www/sites
	methods DELETE

server
	listen 80
	server_name hello.com www.hello.com hello_world.com www.hello_world.com
	root /var/www/hello
	methods INVALID

server
	listen   
	root /var/www/abc
	server_name abc.com 
	methods GET

